package com.medicine.exception;

/**
 * @Auther: 刘涵
 * @Date: 2019-12-10 21:25
 * @Description:库存异常
 */
public class WareHouseStockException extends RuntimeException  {
    public WareHouseStockException() {
    }

    public WareHouseStockException(String message) {
        super(message);
    }

    public WareHouseStockException(String message, Throwable cause) {
        super(message, cause);
    }

    public WareHouseStockException(Throwable cause) {
        super(cause);
    }

    public WareHouseStockException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(message, cause, enableSuppression, writableStackTrace);
    }
}
