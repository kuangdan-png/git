package com.medicine.pojo;

/**
 * @Auther: 刘涵
 * @Date: 2019-12-11 15:35
 * @Description:员工角色信息表
 */
public class EmployeesRole {

    private String empRolNo;
    private String empNo;
    private String rolNo;
    private String empRolRemark;

    @Override
    public String toString() {
        return "EmployeesRole{" +
                "empRolNo='" + empRolNo + '\'' +
                ", empNo='" + empNo + '\'' +
                ", rolNo='" + rolNo + '\'' +
                ", empRolRemark='" + empRolRemark + '\'' +
                '}';
    }

    public EmployeesRole(String empRolNo, String empNo, String rolNo, String empRolRemark) {
        this.empRolNo = empRolNo;
        this.empNo = empNo;
        this.rolNo = rolNo;
        this.empRolRemark = empRolRemark;
    }

    public EmployeesRole() {
    }

    public void setEmpRolNo(String empRolNo) {
        this.empRolNo = empRolNo;
    }

    public void setEmpNo(String empNo) {
        this.empNo = empNo;
    }

    public void setRolNo(String rolNo) {
        this.rolNo = rolNo;
    }

    public void setEmpRolRemark(String empRolRemark) {
        this.empRolRemark = empRolRemark;
    }
}
